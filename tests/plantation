game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Welcome!";
                Text = "Enjoy using Doge Hub";
                Duration = 1.5;})


local Library = loadstring(game:HttpGet("https://pastebin.com/raw/GdN5iRbt"))() 
local Window = Library.CreateLib("NIGGER", "DarkTheme") 


local Tab1 = Window:NewTab("Autofarm")
local Tab1Section = Tab1:NewSection("Scripts")



function tp(pos) 
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end
function killall()
    for i, v in pairs(game.Players:GetPlayers()) do
        spawn(function()
            pcall(function()
                local ohVector31 = Vector3.new(-186.46624755859375, 49.74998474121094, math.random(-49.323232, 49.488882))
                local ohVector32 = Vector3.new(-254.47802734375, 68.99893188476562, math.random(-49.323232, 49.488882))
                local ohInstance3 = v.Character.LowerTorso
                local ohVector34 = Vector3.new(-222.7018585205078, 60.864871978759766, math.random(-49.323232, 49.488882))
                game:GetService("ReplicatedStorage").Remotes.Shoot:FireServer(ohVector31, ohVector32, ohInstance3, ohVector34)
            end)
        end)
    end
end
function equipgun()
    for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if v:FindFirstChild("Fire") then
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
            break
        end
    end
end








Tab1Section:NewButton("Farm 1 - Left", "Load.", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Success.";
                Duration = 0.1;})

while true do
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(72.9500732421875, -310.907470703125, -104.05709075927734)
wait (1)
end

end)

Tab1Section:NewButton("Farm 2 - Right", "Load.", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Success.";
                Duration = 0.1;})

while true do
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(79.58275604248047, -310.907470703125, -103.96561431884766)
wait (1)
end

end)

Tab1Section:NewButton("Auto Kill", "Load.", function()
game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "Notification";
                Text = "Success.";
                Duration = 0.1;})

while true do
equipgun()
killall()
wait(0.1)
end

end)

